if [ ! -f intel-vaapi-driver-2.3.0.tar.bz2 ];then
wget -c https://github.com/intel/intel-vaapi-driver/releases/download/2.3.0/intel-vaapi-driver-2.3.0.tar.bz2 --no-check-certificate
fi
if [ -f intel-vaapi-driver-2.3.0.tar.bz2 ];then
echo ''
echo 'NIL'
echo 'NIL' > ./R
else
echo ''
echo 'GET-ERROR'
echo 'GET-ERROR' > ./R
fi
