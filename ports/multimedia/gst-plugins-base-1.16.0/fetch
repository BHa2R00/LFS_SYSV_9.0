if [ ! -f gst-plugins-base-1.16.0.tar.xz ];then
wget -c https://gstreamer.freedesktop.org/src/gst-plugins-base/gst-plugins-base-1.16.0.tar.xz --no-check-certificate
fi
if [ -f gst-plugins-base-1.16.0.tar.xz ];then
echo ''
echo 'NIL'
echo 'NIL' > ./R
else
echo ''
echo 'GET-ERROR'
echo 'GET-ERROR' > ./R
fi
