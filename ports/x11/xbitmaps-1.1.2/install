if [ -f xbitmaps-1.1.2.tar.bz2 ];then
tar xf xbitmaps-1.1.2.tar.bz2
cd xbitmaps-1.1.2
export XORG_PREFIX="$PORTS_DEST"
export XORG_CONFIG="--prefix=$XORG_PREFIX --sysconfdir=/etc \
    --localstatedir=/var --disable-static"
./configure $XORG_CONFIG
make install
echo ''
echo 'NIL'
echo 'NIL' > ./R
else
echo ''
echo 'BUILD-ERROR'
echo 'BUILD-ERROR' > ./R
fi
