if [ -f xclock-1.0.9.tar.bz2 ];then
tar xf xclock-1.0.9.tar.bz2
cd xclock-1.0.9
export XORG_PREFIX="$PORTS_DEST"
export XORG_CONFIG="--prefix=$XORG_PREFIX --sysconfdir=/etc \
    --localstatedir=/var --disable-static"
./configure $XORG_CONFIG
make $PORTS_JOBS
make install
echo ''
echo 'NIL'
echo 'NIL' > ./R
else
echo ''
echo 'BUILD-ERROR'
echo 'BUILD-ERROR' > ./R
fi
