if [ ! -f seamonkey-2.49.4.source.tar.xz ];then
wget -c https://archive.mozilla.org/pub/seamonkey/releases/2.49.4/source/seamonkey-2.49.4.source.tar.xz --no-check-certificate
fi
if [ -f seamonkey-2.49.4.source.tar.xz ];then
echo ''
echo 'NIL'
echo 'NIL' > ./R
else
echo ''
echo 'GET-ERROR'
echo 'GET-ERROR' > ./R
fi
