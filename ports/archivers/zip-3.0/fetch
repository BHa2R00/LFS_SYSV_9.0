if [ ! -f zip30.tar.gz ];then
wget -c https://downloads.sourceforge.net/infozip/zip30.tar.gz --no-check-certificate
#wget -c ftp://ftp.info-zip.org/pub/infozip/src/zip30.tgz --no-check-certificate
fi
if [ -f zip30.tar.gz ];then
echo ''
echo 'NIL'
echo 'NIL' > ./R
else
echo ''
echo 'GET-ERROR'
echo 'GET-ERROR' > ./R
fi
